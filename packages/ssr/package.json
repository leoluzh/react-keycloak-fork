{
  "name": "@leoluzh/react-keycloak-fork-ssr",
  "version": "1.3.2",
  "license": "MIT",
  "description": "Fork of https://github.com/react-keycloak/react-keycloak.git with updated packages for Keycloak >=19. SSR bindings for Keycloak javascript adapter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leoluzh/react-keycloak-fork.git"
  },
  "author": "leoluzh",
  "bugs": {
    "url": "https://github.com/leoluzh/react-keycloak-fork/issues"
  },
  "homepage": "https://github.com/leoluzh/react-keycloak-fork#readme",
  "keywords": [
    "react",
    "keycloak",
    "keycloak-js",
    "nextjs",
    "razzle"
  ],
  "main": "lib-commonjs/index.js",
  "jsnext:main": "lib/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "lib-commonjs"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "clean": "rimraf lib lib-commonjs",
    "prebuild": "npm run clean",
    "compile:commonjs": "tsc --module commonjs --outDir lib-commonjs",
    "compile:esnext": "tsc --module esnext --outDir lib",
    "compile": "run-p compile:*",
    "fixmaps:lib": "tweak_sourcemap_paths -a --include 'lib/**/*.js.map' 'lib-commonjs/**/*.js.map'",
    "fixmaps": "run-p fixmaps:*",
    "build": "run-s compile fixmaps",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build && cp ../../LICENSE.md ."
  },
  "peerDependencies": {
    "keycloak-js": ">=26.0.0",
    "react": ">=19.0",
    "typescript": ">=3.8"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.28.6",
    "@leoluzh/react-keycloak-fork-core": "3.0.2",
    "detect-node": "^2.1.0",
    "hoist-non-react-statics": "^3.3.2",
    "js-base64": "^3.7.8",
    "js-cookie": "^3.0.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom"
  },
  "devDependencies": {
    "keycloak-js": "^26.2.3"
  },
  "gitHead": "2bddc6e982dead9d0ab3df388db9efed5da89abd"
}
